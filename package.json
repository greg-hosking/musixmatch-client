{
  "name": "musixmatch-client",
  "version": "0.3.2",
  "description": "TypeScript client for MusixMatch API",
  "main": "dist/cjs/client.js",
  "module": "dist/esm/client.js",
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "prebuild": "npm run test",
    "build": "npm run clean && npm run build:cjs && npm run build:esm"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greg-hosking/musixmatch-client.git"
  },
  "author": "Greg Hosking",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/greg-hosking/musixmatch-client/issues"
  },
  "homepage": "https://github.com/greg-hosking/musixmatch-client#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  }
}
